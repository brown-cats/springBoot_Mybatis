<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.example.mapper.DoctorMapper">    <resultMap id="BaseResultMap" type="com.example.entity.Doctor">        <result column="id" jdbcType="INTEGER" property="id" />        <result column="name" jdbcType="VARCHAR" property="name" />        <result column="age" jdbcType="INTEGER" property="age" />        <result column="sex" jdbcType="VARCHAR" property="sex" />        <result column="office" jdbcType="VARCHAR" property="office" />    </resultMap>    <select id="selById" resultType="com.example.entity.Doctor">        select * from doctor where id = #{id}    </select>    <select id="selByName" resultType="com.example.entity.Doctor">        select * from doctor        <where>            name like "%"#{name ,jdbcType=VARCHAR}"%"        </where>    </select>    <!-- 返回值为List<Doctor>，resultType为Doctor-->    <select id="findAllDoctor" resultType="com.example.entity.Doctor">        select * from doctor    </select></mapper>