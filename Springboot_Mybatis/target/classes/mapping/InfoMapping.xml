<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.example.mapper.InfoMapper">    <resultMap id="BaseResultMap" type="com.example.entity.Info">        <result column="id" jdbcType="INTEGER" property="id" />        <result column="icon" jdbcType="BLOB" property="icon" typeHandler="org.apache.ibatis.type.BlobTypeHandler" />        <result column="introduction" jdbcType="LONGVARCHAR" property="introduction" />    </resultMap>    <resultMap id="idAndIcon" type="com.example.entity.Info">        <result column="id" jdbcType="INTEGER" property="id" />        <result column="icon" jdbcType="BLOB" property="icon" typeHandler="org.apache.ibatis.type.BlobTypeHandler" />    </resultMap>    <select id="getBlobById" resultMap="BaseResultMap">        select * from doctorinfo where id = #{id}    </select>    <select id="getAllBlob" resultMap="BaseResultMap">        select * from doctorinfo    </select>    <select id="getClobById" resultType="String">        select introduction from doctorinfo where id = #{id}    </select></mapper>